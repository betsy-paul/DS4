{
    "manifest_version": 3,
    "name": "20-20-20",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "html/popup.html"
    },
    "background": {
      "service_worker": "js/background.js"
    },
    "short_name": "Triple Twenty",
    "permissions": [
      "tabs",
      "storage",
      "alarms"
    ],
  "browser_action": {
    "default_icon": ".assets/download.png"
  }
}
